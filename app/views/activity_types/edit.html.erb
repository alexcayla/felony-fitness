<div class="container">


<h1>Edit activity_type</h1>
<%= form_for(@activity_type) do |f| %>
<div class="form-group">
  <%= f.label :name %>
  <%= f.text_field :name, class: 'form-control' %>
</div>

<div class="form-group">
  <%= f.label :description %>
  <%= f.text_field :description, class: 'form-control' %>
</div>

<%= f.submit class: 'btn btn-primary' %>
<% end %>
</div>
